{
  "name": "@maxhill/components",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./custom-elements.json": "./custom-elements.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "custom-elements.json"
  ],
  "scripts": {
    "build": "cem analyze && vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "docs": "cem analyze",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@maxhill/css": "workspace:*"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.10",
    "@vitest/browser": "^4.0.6",
    "glob": "^11.0.3",
    "happy-dom": "^20.0.10",
    "rollup-plugin-string": "^3.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.11",
    "vitest": "^4.0.6"
  },
  "customElements": "custom-elements.json"
}
