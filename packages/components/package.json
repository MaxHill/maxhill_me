{
  "name": "@maxhill/components",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/register-all.js",
  "module": "dist/register-all.js",
  "types": "dist/register-all.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./custom-elements.json": "./dist/custom-elements.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && cem analyze",
    "dev": "vite build --watch & cem analyze --watch",
    "clean": "rm -rf dist",
    "docs": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@maxhill/css": "workspace:*"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.10",
    "@vitest/browser": "^4.0.6",
    "@wc-toolkit/cem-validator": "^1.0.3",
    "@wc-toolkit/jsdoc-tags": "^1.1.0",
    "glob": "^11.0.3",
    "happy-dom": "^20.0.10",
    "rollup-plugin-string": "^3.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.11",
    "vitest": "^4.0.6"
  },
  "customElements": "dist/custom-elements.json"
}
