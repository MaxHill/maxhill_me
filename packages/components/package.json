{
  "name": "@maxhill/components",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/register-all.js",
  "module": "dist/register-all.js",
  "types": "dist/register-all.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./custom-elements.json": "./dist/custom-elements.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && cem analyze",
    "dev": "vite build --watch & cem analyze --watch",
    "clean": "rm -rf dist",
    "docs": "pnpm run build",
    "test": "web-test-runner",
    "test:watch": "web-test-runner --watch"
  },
  "dependencies": {
    "@maxhill/css": "workspace:*"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.10",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@open-wc/testing": "3.0.0-next.5",
    "@open-wc/testing-helpers": "2.0.0-next.3",
    "@types/mocha": "^10.0.10",
    "@wc-toolkit/cem-validator": "^1.0.3",
    "@web/dev-server-esbuild": "^1.0.4",
    "@web/test-runner": "^0.20.2",
    "@web/test-runner-playwright": "^0.11.1",
    "glob": "^11.0.3",
    "rollup-plugin-string": "^3.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.11"
  },
  "customElements": "dist/custom-elements.json"
}
